{
  "name": "typedoc-wrapper",
  "version": "6.0.1",
  "description": "Wrapper for typedoc and typedoc-plugin-custom-tags",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tdx": "bin/index.js"
  },
