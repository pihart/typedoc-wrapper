  "bugs": {
    "url": "https://github.com/pihart/typedoc/issues"
  },
  "homepage": "https://github.com/pihart/typedoc#readme",
  "devDependencies": {
    "@types/node": "^16.11.11",
    "typedoc": "^0.21.0",
    "typedoc-plugin-custom-tags": "github:pihart/typedoc-plugin-custom-tags#pihart-dist-v1.1.0",
    "typescript": "^4.1.3"
